int aleatorio = 100;
void setup() {
pinMode(8, OUTPUT);
pinMode(9, OUTPUT);
pinMode(10, OUTPUT);
pinMode(11, OUTPUT);
pinMode(12, OUTPUT);

}

void loop() {
digitalWrite(8, 1);
delay(aleatorio);
digitalWrite(8, 0);
delay(aleatorio);
digitalWrite(9, 1);
delay(aleatorio);
digitalWrite(9, 0);
delay(aleatorio);
digitalWrite(10, 1);
delay(aleatorio);
digitalWrite(10, 0);
delay(aleatorio);
digitalWrite(11, 1);
delay(aleatorio);
digitalWrite(11, 0);
delay(aleatorio);
digitalWrite(12, 1);
delay(aleatorio);
digitalWrite(12, 0);
delay(aleatorio);
digitalWrite(11, 1);
delay(aleatorio);
digitalWrite(11, 0);
delay(aleatorio);
digitalWrite(10, 1);
delay(aleatorio);
digitalWrite(10, 0);
delay(aleatorio);
digitalWrite(9, 1);
delay(aleatorio);
digitalWrite(9, 0);
delay(aleatorio);

}
